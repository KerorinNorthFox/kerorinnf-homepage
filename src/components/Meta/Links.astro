---
const canonicalURL = new URL(Astro.url.pathname, Astro.site);
---
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
<link rel="canonical" href={canonicalURL} />
<!-- Font preloads -->
<link rel="preload" href="/fonts/hackgen/HackGen-Regular.ttf" as="font" type="font/ttf" crossorigin />
<link rel="preload" href="/fonts/hackgen/HackGen-Bold.ttf" as="font" type="font/ttf" crossorigin />
